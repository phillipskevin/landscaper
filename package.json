{
  "name": "landscaper",
  "version": "0.0.0",
  "description": "Maintenance help for your apps.",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phillipskevin/landscaper.git"
  },
  "author": {
    "name": "Kevin Phillips",
    "email": "kphillips86@gmail.com",
    "url": ""
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "node build",
    "develop": "done-serve --develop --port 8080 --proxy http://localhost:3030",
    "lint": "eslint src/**/*.js src/*.js",
    "start": "done-serve --port 8080 --proxy http://localhost:3030",
    "test": "npm run lint && npm run testee",
    "testee": "testee test.html --browsers firefox --reporter Spec"
  },
  "main": "landscaper/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [],
  "steal": {
    "main": "landscaper/index.stache!done-autorender",
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "directories": {
      "lib": "src"
    },
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "can-component": "^3.0.4",
    "can-connect": "^1.0.21",
    "can-connect-feathers": "^3.5.2",
    "can-define": "^1.0.10",
    "can-route": "^3.0.6",
    "can-route-pushstate": "^3.0.1",
    "can-set": "^1.0.3",
    "can-stache": "^3.0.16",
    "can-view-autorender": "^3.0.3",
    "can-zone": "^0.6.0",
    "done-autorender": "^1.0.0-alpha.0",
    "done-component": "^0.6.0-pre.5",
    "done-css": "^3.0.0-alpha.0",
    "done-serve": "^1.0.0-alpha.0",
    "feathers": "^2.1.1",
    "feathers-authentication-client": "^0.3.1",
    "feathers-authentication-popups": "^0.1.2",
    "feathers-hooks": "^1.8.1",
    "feathers-socketio": "^1.5.2",
    "generator-donejs": "^1.0.0-alpha.0",
    "steal": "^1.0.5",
    "steal-less": "^1.0.2",
    "steal-platform": "0.0.4",
    "steal-socket.io": "^4.0.9",
    "steal-stache": "^3.0.4"
  },
  "devDependencies": {
    "can-fixture": "^1.0.11",
    "can-view-model": "^3.1.3",
    "donejs-cli": "^1.0.0-alpha.2",
    "eslint": "^3.17.1",
    "funcunit": "^3.1.0",
    "steal-qunit": "^1.0.0",
    "steal-tools": "^1.0.1",
    "testee": "^0.3.0"
  }
}
